    @media screen and (max-width: 1024px) { 
      body {
        min-height: 100vh;
      }

      .container {
        margin-top: 0;
        align-items: center;
        justify-content: center;
      }

      .container .message {
        padding: 0 20px;
      }

      .container .error {
        position: fixed;
         display: flex;
        justify-content: center;
        align-items: center;
        width: 100vw;
        height: 100vh;
        top: 0;
        left: 0; 
        transform: none;  
      }

      .container .error span {
        display: block;
        font-size: clamp(8rem,25vw, 14rem);
        transform: translateY(-54px);
      }

      .container .action {
        flex-direction: column;
      }

      .container .quicknode-logo,
      .container .quicknode-logo::after {
        display: none;
      }
    }

    @media screen and (max-width: 320px) { 
      .container .error span {
        transform: translateY(-30px);
      }
    }


