    :root{
      --color-stroke: #006f91a5;
      --color-stroke-highlight: #61cdef;
      --bg-color: #10151B;
    }

    *, ::after, ::before {
      box-sizing: border-box;
      border-width: 0;
      border-style: solid;
      border-color: currentColor;
      margin: 0;
      padding: 0;
    }

    body {
      font-family: 'Inter', sans-serif; 
      background-color:var(--bg-color);
      color: #ffffff;
      display: flex;
      justify-content: center;
      overflow: hidden;
    }

    .noselect {
      -webkit-touch-callout: none; 
      -webkit-user-select: none; 
      -khtml-user-select: none; 
      -moz-user-select: none; 
      -ms-user-select: none;
      user-select: none;
    }

    .container {
      display: flex;
      align-items: center;
      flex-direction: column;
      position: relative;
      z-index: 10;
      margin-top: 217px;
    }

    .container h1 {
      font-weight: 500;
      font-size: 1.35rem;
      line-height: 1.75rem;
      text-align: center;
    }

    .container .message {
      max-width: clamp(50%, 100%, 368px);
      margin-top: 1rem;
      color: #F8F8FA;
      font-size: 13px;
      font-weight: 400;
      text-align: center;
    }

    .container .error {
      position: absolute;
      top: 0;
      left: 50%;
      transform: translate(-50%,-43%);
      z-index: -1;
      color: #00485F;
      font-weight: 700;
      pointer-events: none;
    }

    .container .error span{
      font-size: 20rem;
    }

    .container .action {
      display: flex;
      justify-content: center;
    }

    .container .btn {
      display: block;
      margin: 1.5rem 0.5rem 0;
      font-weight: 400;
      font-size: .75rem;
      line-height: 1rem;
      padding: 0.5rem 1rem;
      border-radius: 0.375rem;
      text-decoration: none;
    }

    .container .btn.return {
      color: #ffffff;
      background-color: #0095C8;
    }

    .container .return:hover {
      background-color: #037ea8;
    }

    .container .btn.level-up {
      color: #000000;
      background-color: #ffffff;
    }

    .container .btn.level-up:hover {
      background-color: #e1e1e1;
    }

    .container .quicknode-logo {
      position: absolute;
      width: 1440px;
      height: 1024px;
      top: -218px;
      z-index: -2;
    }

    .container .quicknode-logo svg {
      display: block;
      margin: 0 auto;
    }

    .container .quicknode-logo svg circle.node {
      fill: var(--color-stroke);
      stroke:var(--color-stroke);
    }
 
    .container .quicknode-logo::after {
      content: "";
      position: fixed;
      left: 0;
      top: 40%;
      height: 60%;
      width: 100%;
      background: -webkit-gradient(linear, left top, left bottom, from(rgba(0,0,0, 0)), to(var(--bg-color)),to(var(--bg-color)));
      background: -o-linear-gradient(rgba(0,0,0, 0), var(--bg-color),  var(--bg-color));
      background: linear-gradient(rgba(0,0,0, 0), var(--bg-color), var(--bg-color));
      pointer-events: none;
    }
    
    #S5-193F-257-62 {
      animation: shoot-1 10s ease-in-out infinite;
    }

    #P257-62 {
      animation: highlight-1 10s linear infinite;
    }

    #S5-193F-191-274 {
      animation: shoot-3 15s ease-in infinite;
    }

